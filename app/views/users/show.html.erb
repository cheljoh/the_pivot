<div id="orders" class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2 outline" >
      <h1>orders</h1>
      <h5 class="rjust"> <%= link_to "view all orders", '#' %> </h5>
      <div class="table-responsive">
        <table class="table table-hover table-condensed table-products table-bordered">
          <thead>
            <tr>
              <th class="text-center">order</th>
              <th class="text-center">date</th>
              <th class="text-center">status</th>
              <th class="text-center">total</th>
              <th></th>
            </tr>
          </thead>
            <tr>
              <td><h4><%= link_to 'order.id', '#' %></h4></td>
              <td><h4>order.date</h4></td>
              <td><h4>order.status</h4></td>
              <td><h4>order.display_total</h4></td>
              <td><h4><%= link_to "details", '#' %></h4></td>
            </tr>
        </table>
      </div>
    </div>
  </div>
</div>
<br>
